<% if (page.photos && page.photos.length) { %>
<div class="article-gallery">
    <% page.photos.forEach(function(photo, i) { %>
    <a class="gallery-item" href="<%- url_for_lang(photo) %>" rel="gallery_<%= page._id %>">
        <img src="<%- url_for_lang(photo) %>" itemprop="image" />
    </a>
    <% }) %>
</div>
<% } %>
